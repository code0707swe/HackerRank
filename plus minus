#include <iostream>

using namespace std;

int main()
{
    int a;
    cin>>a;
    int b[a];
    int p=0,n=0,z=0;
    for (int i=0;i<a;i++){
        cin>>b[i];
        if (b[i]>0)p=p+1;
        else if (b[i]<0)n=n+1;
        else z=z+1;      
      
    }
    cout<<(double)p/a<< endl;
    cout<<(double)n/a<< endl;
    cout<<(double)z/a<< endl;
    
    
}
