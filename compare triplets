#include <iostream>
using namespace std;
int main()
{
    int a[3];
    int b[3];
    for( int i=0; i<3;i++)
    {
        cin>>a[i];
        
    }
    for( int i=0; i<3;i++)
    {
        cin>>b[i];
    }
    int l=0,m=0;
    for(int i=0; i<3;i++)
    {
        if (a[i]>b[i])
            l=l+1;
        else if (a[i]<b[i])
            m=m+1;
        else continue; 
        
    }
    cout<<l<<" "<<m;
}
